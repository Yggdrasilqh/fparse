{
    "name": "fparser",
    "version": "2.0.2",
    "description": "A Math Formula parser library for JavaScript",
    "main": "src/fparser.js",
    "module": "src/fparser.js",
    "type": "module",
    "scripts": {
        "docker-image": "docker build --pull --target develop -t fparser .",
        "docker-shell": "docker run --rm -ti -v \"$PWD\":/usr/src/app -w /usr/src/app fparser bash",
        "docker-demopage": "docker run --rm -ti -v \"$PWD\":/usr/src/app -w /usr/src/app/demopage -p 3000:8080 fparser npm run serve",
        "docker-test": "docker run --rm -ti -v \"$PWD\":/usr/src/app -w /usr/src/app fparse npm run test",
        "build-dev": "NODE_ENV=development webpack",
        "build-demopage-image": "docker build --pull -t fparser-demopage .",
        "build": "NODE_ENV=production webpack",
        "test": "NODE_ENV=development npm run build-dev && jasmine && karma start"
    },
    "keywords": [
        "javascript",
        "formula",
        "parser",
        "formula",
        "parser",
        "mathematical",
        "formula",
        "parser"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/bylexus/fparse.git"
    },
    "author": "Alex Schenkel <alex@alexi.ch>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bylexus/fparse/issues"
    },
    "homepage": "https://fparser.alexi.ch/",
    "devDependencies": {
        "@babel/core": "^7.17.8",
        "@babel/preset-env": "^7.16.11",
        "@vue/cli": "^4.5.17",
        "babel-loader": "^8.2.4",
        "babel-plugin-add-module-exports": "^1.0.4",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^3.4.1",
        "jasmine": "^3.99.0",
        "karma": "^6.3.17",
        "karma-chrome-launcher": "^3.1.1",
        "karma-jasmine": "^4.0.1",
        "prettier": "^2.6.1",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2"
    },
    "eslintConfig": {
        "root": true,
        "extends": "prettier",
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 9
        },
        "env": {
            "browser": true,
            "node": true
        },
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": [
                "error",
                {
                    "singleQuote": true,
                    "tabWidth": 4,
                    "printWidth": 120
                }
            ],
            "no-unused-vars": [
                "error",
                {
                    "vars": "all",
                    "args": "none"
                }
            ],
            "semi": [
                "error",
                "always"
            ],
            "curly": "error",
            "eqeqeq": "error",
            "no-eval": "error",
            "no-loop-func": "error",
            "radix": "error",
            "comma-dangle": "error",
            "no-undef": "error"
        },
        "globals": {
            "describe": false,
            "it": false,
            "beforeEach": false,
            "expect": false,
            "spyOn": false
        }
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "none"
    }
}
